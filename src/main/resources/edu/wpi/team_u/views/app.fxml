        <Button fx:id="laundryService" alignment="CENTER" contentDisplay="CENTER" layoutX="365.0" layoutY="117.0" mnemonicParsing="false" onAction="#toLaundryService" prefHeight="68.0" prefWidth="199.0" text="Laundry Service">
